<h1>Listing main_material_brands</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Logo path</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @main_material_brands.each do |main_material_brand| %>
  <tr>
    <td><%= main_material_brand.name %></td>
    <td><%= main_material_brand.logo_path %></td>
    <td><%= link_to 'Show', main_material_brand %></td>
    <td><%= link_to 'Edit', edit_main_material_brand_path(main_material_brand) %></td>
    <td><%= link_to 'Destroy', main_material_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

